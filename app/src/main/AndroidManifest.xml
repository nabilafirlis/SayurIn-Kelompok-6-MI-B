<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ac.unila.SayurIn">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="id.ac.unila.SayurIn.CartActivity"></activity>
        <activity android:name="id.ac.unila.SayurIn.OrderActivity" />
        <activity android:name="id.ac.unila.SayurIn.SettingActivity" />

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/preferences" />

        <activity android:name="id.ac.unila.SayurIn.MainActivity" />
        <activity android:name="id.ac.unila.SayurIn.LoginActivity" />
        <activity android:name="id.ac.unila.SayurIn.WelcomeActivity" />
        <activity android:name="id.ac.unila.SayurIn.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>